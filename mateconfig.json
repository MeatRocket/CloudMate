{
    "builds": [{
            "name": "dev",
            "js": {
                "minify": false,
                "sourceMap": true,
                "declaration": false
            },
            "ts": {
                "compilerOptions": {
                    "noImplicitAny": false,
                    "noEmitOnError": false,
                    "removeComments": true,
                    "target": "es5"
                }
            }
        },
        {
            "name": "dist",
            "outDir": "dist",
            "js": {
                "minify": false,
                "sourceMap": false,
                "declaration": false
            },
            "ts": {
                "compilerOptions": {
                    "noImplicitAny": false,
                    "noEmitOnError": false,
                    "removeComments": true,
                    "target": "es5"
                }
            }
        }
    ],
    "files": [{
        "input": ["src/clean.ts"],
        "output": ["test/clean.js"]
    }, {
        "input": ["src/config.ts"],
        "output": ["test/config.js"],
        "builds": ["dev", "dist"]
    }, {
        "input": ["src/main.ts"],
        "output": ["test/main.js"],
        "builds": ["dev", "dist"]
    }, {
        "input": ["src/cli.ts"],
        "output": ["test/cli.js"],
        "builds": ["dev", "dist"]
    }]
}