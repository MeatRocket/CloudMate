{
	"format": {
		"path": ["./mateconfig.json"]
	},
	"builds": [
		{
			"name": "dev",
			"js": {
				"minify": false,
				"sourceMap": true,
				"declaration": false
			}
		},
		{
			"name": "dist",
			"outDir": "dist",
			"js": {
				"minify": false,
				"sourceMap": false,
				"declaration": false
			}
		}
	],
	"files": [
		{
			"input": ["src/clean.ts"],
			"output": ["test/clean.js"]
		},
		{
			"input": ["src/config.ts"],
			"output": ["test/config.js"],
			"builds": ["dev", "dist"]
		},
		{
			"input": ["src/bundler.ts"],
			"output": ["test/bundler.js"],
			"builds": ["dev", "dist"]
		},
		{
			"input": ["src/formatter.ts"],
			"output": ["test/formatter.js"],
			"builds": ["dev", "dist"]
		},
		{
			"input": ["src/cli.ts"],
			"output": ["test/cli.js"],
			"builds": ["dev", "dist"]
		},
		{
			"input": ["src/webcleanjs.ts"],
			"output": ["test/webcleanjs.js"],
			"builds": ["dev", "dist"]
		}
	]
}
